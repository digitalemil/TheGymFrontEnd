<head>
    <title>Alle Kurse</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
<body>
  
<h1 style="text-align:center">Access restricted.</h1>
<a href="/app/home"><img id="img" class="icon"/></a>
  
<pre></pre>
<script> 
  function loadCSS() {     
    console.log("Loading CSS");     
    var head = document.getElementsByTagName('HEAD')[0];  
    var link = document.createElement('link'); 
    link.rel = 'stylesheet';      
    link.type = 'text/css'; 
    link.href = "../stylesheets/"+"<%= global.darkmode?'style-dark.css':'style.css' %>";  
    head.appendChild(link);  
   document.getElementById("img").src='<%= global.darkmode?"../images/logo-512-transparentbg-grey.png":"../images/logo-512-whitebg.png" %>'
  };
  loadCSS();
</script>  
</body>